  "author": "Matthias Auer (mail@matthias-auer.net)",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/sinon": "^1.16.31",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "expose-loader": "^0.7.1",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.1.2",
    "mocha-typescript": "^1.0.11",
    "phaser": "^2.6.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "ts-loader": "^0.9.5",
    "ts-node": "^1.6.0",
    "typescript": "^2.0.6",
    "typescript-collections": "^1.2.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
