{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "description": "an implementation of Tic-Tac-Toe using Typescript and Phaser",
  "main": "dist/app.bundle.js",
  "scripts": {
    "test": "webpack --progress --display-error-details . && mocha --opts mocha.opts",
